# Exemplo de arquivo .env para Wilb - Assistente de Estudos IA

# Porta do servidor
PORT=3001

# Configurações do Firebase
FIREBASE_API_KEY=your_firebase_api_key
FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
FIREBASE_APP_ID=your_firebase_app_id

# API Key do Google Gemini
GEMINI_API_KEY=your_gemini_api_key

# Ambiente de execução
NODE_ENV=development

# Domínios permitidos para CORS (separados por vírgula, sem espaços)
CORS_ALLOWED_ORIGINS=https://yourdomain.com,https://staging.yourdomain.com,http://localhost:3000
